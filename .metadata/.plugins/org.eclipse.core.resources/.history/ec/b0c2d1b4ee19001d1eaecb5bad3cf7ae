
public class FindMinAndMax {
public static void main(String[] args) {
	int ar[]= {-12,-7,-5,-18,-3};
	int min=ar[0];// it is from the array
	int max=0;
	//Sorting is done--> NlogN
	//inital min = 0
	for(int i=0;i<ar.length;i++) {
		//3<5
		if(ar[i]<min)
			min=ar[i];
		if(ar[i]>max)
			max=ar[i];
	
	}
	System.out.println("min "+min);
	System.out.println("max "+max);
}
}
